		<h1>Shopping App - Log in!</h1>
		<hr class="m-y-2">

	<div class="jumbotron", style="max-width:600px">
		<div class="col-sm4">
<%= form_for(:session, url: login_path, :html => {:class => ""}) do |f| %>
	<div class="form-group">
	<%= f.label "E-mail"%>
	<%= f.email_field :email, :placeholder => "Email", class: "form-control" %>
	</div>
	<div class="form-group">
	<%= f.label "Password"%>
	<%= f.password_field :password, :placeholder => "Password", class: "form-control" %>
	</div>

	<%= f.submit "Log in", class: "btn btn-default" %>
	<% end %>
		</div>
	</div>

		


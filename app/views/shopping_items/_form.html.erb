<%= form_for([@shopping_list, @shopping_list.shopping_items.build]) do |f| %>
	<%= f.text_field :content, placeholder: "New Item" %>
	<%= f.text_field :quantity, placeholder: "Qty" %>
	<%= f.text_field :price, placeholder: "Price" %>
	<%= f.text_field :buyer, placeholder: "Buyer" %>
	<%= f.submit %>
<% end %>

